                },
                "name": "[toLower(parameters('cosmosDBname'))]"
            }
        },

        {
            "type": "Microsoft.Compute/virtualMachines/extensions",
            "apiVersion": "2015-06-15",
            "name": "[concat(variables('vmName'), '/', 'extension')]",
            "location": "[variables('location')]",
            "dependsOn": [
                "[concat('Microsoft.Compute/virtualMachines/', variables('vmName'))]"
            ],
            "properties": {
                "publisher": "Microsoft.Compute",
                "type": "CustomScriptExtension",
                "typeHandlerVersion": "1.8",
                "autoUpgradeMinorVersion": true,
                "settings": {
                    "fileUris": [
                        "https://praveeenstoreroomac.blob.core.windows.net/scriptforinstall/installsoftwares.ps1"
                    ] 
                },

                "protectedSettings": {
                    "commandToExecute": "powershell.exe -ExecutionPolicy Unrestricted -File installsoftwares.ps1"
                }
            }
        }
       
    ],

    "outputs": {

        "DeploymentID": {
            "type": "String",
            "value": "[ parameters('DeploymentID')]"
        },
    
        "LABVM Admin Username": {
            "type": "String",
            "value": "[parameters('adminUsername')]"
        },
    
        "LABVM Admin Password": {
            "type": "String",
            "value": "[parameters('adminPassword')]"
        },
    
        "LABVM DNS Name": {
            "type": "String",
