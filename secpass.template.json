{  
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    
    "parameters":{
        "adminUsername": {
            "type": "string",
            "metadata": {
                "description": "Username of the VM"
            }
        },

        "adminPassword": {
            "type": "securestring",
            "metadata": {
                "description": "password for VM"
            }
        },

        "AzureUsername": {
            "type": "string",
            "metadata": {
                "description": "username of the Azure account"
            }
        },

        "Azurepassword": {
            "type": "securestring",
            "metadata": {
                "description": "Password of the Azure account"
            }
        },

        "ODLID": {
            "type": "string",
            "metadata": {
                "description": "ID of On demand lab of cloudlabs"
            }
        },

        "DeploymentID": {
            "type": "string",
            "metadata": {
                "description": "Deployment ID of the lab in cloudlab (DID)"
            }
        },

        "sqlServername": {
            "type": "string",
            "defaultValue": "paassecserver",
            "metadata": {
                "description": "Name for the sql Server."
            }
        },

        "cosmosDBname": {
            "type": "string",
            "defaultValue": "paasseccosmosdb",
            "metadata": {
                "description": "Name for the Cosmos DB account"
            }
        },

        "appserviceenvname": {
            "type": "string",
            "defaultValue": "paassecasee",
            "metadata": {
                "description": "Name for the app service environment"
            }
        },

        "storageAccname": {
            "type": "string",
            "defaultValue": "paassecstorage",
            "metadata": {
                "description": "description"
            }
        }

    },

    "variables": {
        "AzureSubscriptionID": "[subscription().subscriptionId]",
        "AzureTenantID": "[subscription().tenantId]",
        "rgName": "[resourceGroup().name]",
        "location": "[resourceGroup().location]",
        "applicationGatewayname": "paassec-waf",
        "virtualNetworkName": "paassec-vnet-main",
        "databaseName": "paassecdb",
        "publicIpAddressDNSName": "[concat('paassec-', uniqueString(resourceGroup().id))]",
        "publicIpAddressName": "[concat('wafpip', uniqueString(resourceGroup().id))]",
        "vmName": "paassecvm",
        "subnetName": "waf",
        "subnetRef": "[resourceId('Microsoft.Network/virtualNetworks/subnets/', variables('virtualNetworkName'), variables('subnetName'))]",
        "mainSubnetRef": "[resourceId('Microsoft.Network/virtualNetworks/subnets/', variables('virtualNetworkName'), 'default')]",
        "publicIPRef": "[resourceId('Microsoft.Network/publicIPAddresses', variables('publicIPAddressName'))]",
        "applicationGatewayID": "[resourceId('Microsoft.Network/applicationGateways', variables('applicationGatewayName'))]"
    },

    "resources": [

        {
